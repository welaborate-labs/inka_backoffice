<div class="columns is-centered mt-6">
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th class="has-text-right"> <%= link_to "+", new_identities_path, class: "tag is-large is-primary" %> </th>
      <th colspan="2"></th>
    </tr>
  </thead>
  <tbody>
    <div id="users">
      <% @users.each do |user| %>
        <tr>
          <%= render user %>
        </tr>
      <% end %>
    </div>
  </tbody>
</table>
</div>
