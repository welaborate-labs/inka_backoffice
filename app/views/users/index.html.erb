<h1>USER</h1>
<%= link_to "Add new user", new_identities_path %>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>QRCode</th>
      <th colspan="2"></th>
    </tr>
  </thead>
  <tbody>
    <div id="users">
      <% @users.each do |user| %>
        <tr>
          <%= render user %>
        </tr>
      <% end %>
    </div>
  </tbody>
</table>
